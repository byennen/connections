= content_for :header do
  .span3
    = link_to "javascript:void(0);", class: 'mono mn' do
      = display_image(@university.image, "xxlarge")#image_tag @university.image.xxlarge, class: "circular"
  .span7
    .carus
      %ul#events.flexisel
        - @university_events.each do |event|
          %li
            = link_to university_university_event_path(@university, event) do
              = image_tag(event.image)
            %br
            %h4
              = event.title
            %p
              = event.date
              %br
              = event.time
        - @clubs.each do |club|
          - club.events.each do |club_event|
            %li
              = link_to university_club_club_event_path(@university, club, club_event) do
                = image_tag(club_event.image)
              %br
              %h4
                = club_event.title
              %p
                = club_event.date
                %br
                = club_event.time
  .clearfix

.row-fluid
  .container
    .span10.offset2.top2
      %ul.tabs3
        %li= link_to "Community", "#tab1"
        %li= link_to "Clubs", "#tab2"
        %li= link_to "Free Food", "#tab3"
        %li= link_to "Alumni Map", "#tab4"
        %li= link_to "Register New", "#tab5"
      .clearfix
      .tab_container
        #tab1.tab_content
          .filters
            %p Filter by:
            = link_to "All", "javascript:;", :"data-toggle" => ".users-list", :"data-target" => "#all-users", class: 'toggle'
            = link_to "Students", "javascript:;", :"data-toggle" => ".users-list", :"data-target" => "#students", class: 'toggle'
            = link_to "Alumni", "javascript:;", :"data-toggle" => ".users-list", :"data-target" => "#alumnis", class: 'toggle'
          %ul#all-users.two-users.users-list
            - @users.each do |user|
              %li{style: "margin-left: 0"}
                = link_to user_path(user) do
                  = display_image user.profile.image, 'thumb'
                .span4
                  %h4= user.full_name
                  %br
                  = user.graduation_year
          %ul#students.two-users.users-list.hide
            - @users.student.each do |user|
              %li{style: "margin-left: 0"}
                = link_to user_path(user) do
                  = display_image user.profile.image, 'thumb'
                .span4
                  %h4= user.full_name
                  %br
                  = user.graduation_year
          %ul#alumnis.two-users.users-list.hide
            - @users.alumni.each do |user|
              %li{style: "margin-left: 0"}
                = link_to user_path(user) do
                  = display_image user.profile.image, 'thumb'
                .span4
                  %h4= user.full_name
                  %br
                  = user.graduation_year

        #tab2.tab_content.list_carousel
          %ul#foo2.carou-fred-sel
            - item_mar = ""
            - item_mar = "margin-left: 100px;" if @clubs.size == 2
            - item_mar = "margin-left: 300px;" if @clubs.size == 1
            - @clubs.each do |club|
              %li{style: item_mar}
                .users
                  = link_to university_club_path(@university, club) do
                    = display_image club.image, 'xlarge'
                    %span.fleft= club.name

          %a.prev#prev2{href: "#"} &lt;
          %a.next#next2{href: "#"} &gt;
          %center
            %buttuon.btn.btn-success.btn3{name: 'btn'} See all
        #tab3.tab_content.span10{style: "margin-left: 60px;"}
          %h3 Free Food Events:
          %ul#free-food.flexisel
            - unless @free_food_events.nil?
              - @free_food_events.each do |event|
                %li
                  = link_to university_university_event_path(@university, event) do
                    = display_image_square event.image, "large"
                  .span10.left3
                    %h4= event.title
                    %p= event.date
                    %p= event.time
              - @clubs.each do |club|
                - club.events.each do |club_event|
                  %li
                    = link_to university_club_club_event_path(@university, club, club_event) do
                      = display_image_square club_event.image, "large"
                    .span10.left3
                      %h4= club_event.title
                      %p= club_event.date
                      %p= club_event.time

        #tab4.tab_content
.clearfix
.row-fluid
  .container
    .span10.offset2
      %ul.tabs4
        %li= link_to "Club Updates", "#tabss1"
        %li= link_to "College Updates", "#tabss"
      .tab_container
        #tabss1.tab_content2
          %ul.posts.newp
            - @club_updates.each do |update|
              %li
                = link_to "javascript:void(0);", class: 'avatr' do
                  %span{style: "padding-top: 0"}
                    - if update.image.blank?
                      No Image
                    - else
                      = image_tag update.image.thumb
                %h4= update.headline
                %p= update.body
                = link_to time_ago_in_words(update.created_at) + " ago", "javascript:void(0);", class: "com ncom"
                .clearfix
        #tabss.tab_content2
          %ul.posts.newp
            - @updates.each do |update|
              %li
                = link_to "javascript:void(0);", class: 'avatr' do
                  %span{style: "padding-top: 0"}
                    - if !update.image.blank?
                      No Image
                    - else
                      = image_tag update.image.thumb
                %p= update.body
                = link_to time_ago_in_words(update.created_at) + " ago", "javascript:void(0);", class: "com ncom"
                .clearfix
