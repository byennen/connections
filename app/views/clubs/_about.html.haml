.eboard.col-lg-12.pull-left
  %h3 eBoard
  %ul.abo
    - @club.admins.each do |admin|
      %li
        = link_to user_path(admin), width: '55' do
          = display_image admin.profile.image, "thumb"
        %strong
          = link_to admin.name, user_path(admin)
        %small
          = @club.user_id == admin.id ? 'Founder' : 'Admin'
          %br/
          Law 2014
%p.mis
  Mission Statement
  %br/
  %br/
  = @club.description.html_safe